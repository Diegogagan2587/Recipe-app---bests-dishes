<%='you are trying to add a book'%>
<%= form_with model: @food, url: {action: "create"} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%=f.label :measurement_unit%>
  <%=f.text_field :measurement_unit%>

  <%=f.label :price%>
  <%=f.text_field :price%>

    <%=f.label :quantity%>
    <%=f.text_field :quantity%>

  <%= f.submit %>
  <%= link_to 'Cancel', foods_path, class: 'btn btn-danger' %>
<% end %>